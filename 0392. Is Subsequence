class Solution {
public:
    bool isSubsequence(string s, string t) {
        int sLen = s.length();
        int tLen = t.length();
        
        if(tLen < sLen) return false;
        
        int j = 0;
        for(int i = 0;i < tLen;i++){
            if(s[j] == t[i]){
                j++;
            }
            if(j == sLen) break;
        }
        return j == sLen;
    }
};
